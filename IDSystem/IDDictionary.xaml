    <SolidColorBrush x:Key="DefaultBorderOff"       Color="#FFD9D9D9" />
    <SolidColorBrush x:Key="DefaultBorderBox"       Color="#FFD8D8D8" />
    <SolidColorBrush x:Key="DefaultForeground"      Color="#FF444444" />
